
public class VarsTest {
	
	public static void main(String[] args) {
		System.out.println("Hello new test");
		
		//Int
		int age;
		age = 37;
		System.out.println(age);
		
		age = 30 + 10;
		System.out.println(age);
		
		age = (7 * 5) + 2;
		System.out.println(age);
		
		System.out.println("the age is: " + age + " congratulations!!!");
		
		//Double
		double salary;
		
	}
}
